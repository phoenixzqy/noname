<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <staticContent>
      <!-- Remove the existing .ts mapping (video/mp2t) -->
      <remove fileExtension=".ts" />
      <!-- Add .ts as JavaScript for TypeScript files -->
      <mimeMap fileExtension=".ts" mimeType="application/javascript" />
    </staticContent>
  </system.webServer>
</configuration>
